{
  "title": "Contractor Plant and Machinery",
  "breadCrumbs": [
    {
      "title": "Dashboard",
      "link": "/login/#/dashboard"
    },
    {
      "title": "Engineering",
      "link": "/login/#/dashboard/engineering"
    },
    {
      "title": "New Quote",
      "link": "/new-quote",
      "isActive": true
    }
  ],
  "sections": [
    {
      "title": "Quotation",
      "isCollapsible": true,
      "isExpandedByDefault": true,
      "subsections": [
        {
          "title": "Contractor Plant and Machinery Insurance",
          "fields": [
            {
              "type": "input",
              "name": "imd_code",
              "label": "Intermediary",
              "placeholder": "Enter Intermediary",
              "validators": {
                "required": true
              },
              "errors": {
                "required": "Intermediary is required."
              },
              "events": [
                { "name": "change", "action": "validateIntermediary" }
              ],
              "resetsFields": ["proposition_name", "policy_transaction_type"]
            },
            {
              "type": "select",
              "name": "proposition_name",
              "label": "Proposition Selection",
              "placeholder": "Select proposition selection",
              "optionsKey": "propositionOptions",

              "validators": {},
              "errors": {
                "required": "Proposition selection is required."
              },
              "events": [{ "name": "change", "action": "onPropositionChange" }],
              "resetsFields": ["machineryDetails"]
            },
            {
              "type": "select",
              "name": "policy_transaction_type",
              "label": "Transaction Type",
              "optionsKey": "transactionTypeOptions",
              "placeholder": "Select transaction type",
              "resetsFields": ["machineryDetails"],
              "events": [
                { "name": "change", "action": "onTransactionTypeChange" }
              ],
              "validators": {
                "required": true
              },
              "errors": {
                "required": "Transaction Type is required."
              }
            },
            {
              "type": "input",
              "name": "proposer_name_primary_insured",
              "label": "Customer First Name",
              "placeholder": "Enter customer first name",
              "validators": {
                "required": true,
                "minLength": 2
              },
              "errors": {
                "required": "Customer first name is required.",
                "minlength": "Customer first name must be at least {requiredLength} characters long"
              }
            },
            {
              "type": "input",
              "name": "mobile_no",
              "label": "Customer Contact Number",
              "placeholder": "Enter customer contact number",
              "validators": {
                "minLength": 10,
                "maxLength": 10,
                "pattern": "MOBILE"
              },
              "mask": "PHONE",
              "errors": {
                "minlength": "Invalid Customer Contact number",
                "maxlength": "Invalid Customer Contact number",
                "pattern": "Invalid Customer Contact number"
              }
            },
            {
              "type": "input",
              "inputType": "email",
              "name": "email",
              "label": "Proposer Email",
              "placeholder": "Enter proposer email",
              "validators": {
                "required": true,
                "pattern": "EMAIL"
              },
              "errors": {
                "required": "Proposer email is required.",
                "pattern": "Please enter a valid email address."
              }
            },
            {
              "type": "input",
              "inputType": "text",
              "name": "existing_policy",
              "label": "Existing Policy No",
              "placeholder": "Enter existing policy number",
              "visibleWhen": {
                "or": [
                  {
                    "field": "policy_transaction_type",
                    "value": "Market Renewal"
                  },
                  { "field": "policy_transaction_type", "value": "RS Renewal" },
                  {
                    "field": "policy_transaction_type",
                    "value": "Deviated Renewal"
                  }
                ]
              },
              "errors": {
                "required": "Existing Policy Number is required."
              },
              "events": [
                { "name": "change", "action": "onExistingPolicyNoChange" }
              ]
            },
            {
              "type": "select",
              "name": "existingInsurer",
              "label": "Existing Insurer",
              "placeholder": "Select existing insurer",
              "visibleWhen": {
                "or": [
                  {
                    "field": "policy_transaction_type",
                    "value": "Market Renewal"
                  }
                ]
              },
              "optionsKey": "existingInsurerOptions",
              "errors": {
                "required": "Existing Insurer is required"
              }
            },
            {
              "type": "select",
              "name": "existing_product",
              "label": "Existing Product",
              "placeholder": "CPM Base Cover",
              "optionsKey": "productOptions",
              "visibleWhen": {
                "or": [
                  {
                    "field": "policy_transaction_type",
                    "value": "Deviated Renewal"
                  },
                  { "field": "policy_transaction_type", "value": "RS Renewal" }
                ]
              },
              "errors": {
                "required": "Existing Product is required."
              }
            },
            {
              "type": "input",
              "inputType": "text",
              "name": "partyId",
              "label": "Party Id",
              "placeholder": "Enter Party Id",
              "visibleWhen": {
                "or": [
                  {
                    "field": "policy_transaction_type",
                    "value": "Deviated Renewal"
                  },
                  { "field": "policy_transaction_type", "value": "RS Renewal" }
                ]
              },
              "readonly": true
            },
            {
              "type": "input",
              "inputType": "text",
              "name": "claim",
              "label": "Claim",
              "placeholder": "Enter claim",
              "visibleWhen": {
                "or": [
                  {
                    "field": "policy_transaction_type",
                    "value": "Deviated Renewal"
                  },
                  { "field": "policy_transaction_type", "value": "RS Renewal" }
                ]
              },
              "readonly": true
            },
            {
              "type": "input",
              "inputType": "text",
              "name": "endtNo",
              "label": "Endt No",
              "placeholder": "Enter Endt No",
              "visibleWhen": {
                "or": [
                  {
                    "field": "policy_transaction_type",
                    "value": "Deviated Renewal"
                  },
                  { "field": "policy_transaction_type", "value": "RS Renewal" }
                ]
              },
              "readonly": true
            },
            {
              "type": "datepicker",
              "name": "existingPolicyExpiryDate",
              "label": "Existing Policy Expiry Date",
              "placeholder": "Select expiry date",
              "mask": "DATE",
              "visibleWhen": {
                "or": [
                  {
                    "field": "policy_transaction_type",
                    "value": "Market Renewal"
                  },
                  { "field": "policy_transaction_type", "value": "RS Renewal" },
                  {
                    "field": "policy_transaction_type",
                    "value": "Deviated Renewal"
                  }
                ]
              },
              "readonly": true
            },
            {
              "type": "datepicker",
              "name": "policy_start_date",
              "label": "Policy Start Date",
              "placeholder": "Select policy start date",
              "mask": "DATE",
              "validators": {
                "required": true
              },
              "errors": {
                "required": "Policy start date is required.",
                "minDate": "Date cannot be before {requiredDate}.",
                "maxDate": "Date cannot be after {requiredDate}."
              },
              "events": [
                { "name": "change", "action": "onPolicyStartDateChange" }
              ]
            },
            {
              "type": "datepicker",
              "name": "policy_end_date",
              "label": "Policy End Date",
              "placeholder": "Select policy end date",
              "mask": "DATE",
              "errors": {
                "required": "Proposer email is required.",
                "pattern": "Please enter a valid email address."
              },
              "readonly": true
            },
            {
              "type": "input",
              "name": "sezPrevilege",
              "label": "SEZ Previlege",
              "placeholder": "Enter SEZ previlege",
              "value": "Apply-GST",
              "validators": {
                "required": true
              },
              "readonly": true,
              "errors": {
                "required": "SEZ previlege is required."
              }
            },
            {
              "type": "select",
              "name": "product",
              "label": "Product",
              "placeholder": "Select product",
              "optionsKey": "productOptions",
              "validators": {
                "required": true
              },
              "events": [{ "name": "change", "action": "onProductChange" }],
              "errors": {
                "required": "Product is required."
              }
            }
          ]
        },
        {
          "title": "Branch Details",
          "fields": [
            {
              "type": "select",
              "name": "pol_serv_branch_name",
              "label": "Branch",
              "placeholder": "Select branch",
              "optionsKey": "branchListOptions",
              "validators": {
                "required": true
              },
              "errors": {
                "required": "Branch is required."
              }
            }
          ]
        }
      ]
    },
    {
      "title": "Risk Details",
      "isCollapsible": true,
      "isExpandedByDefault": true,
      "subsections": [
        {
          "title": "Floater Details",
          "fields": [
            {
              "type": "select",
              "name": "floater",
              "label": "Floater",
              "placeholder": "Select floater",
              "value": "Yes",
              "options": ["Yes", "No"],
              "events": [{ "name": "change", "action": "onFloaterChange" }]
            },
            {
              "type": "select",
              "name": "floater_coverage_in",
              "label": "Floater Coverage Within",
              "placeholder": "Select floater coverage within",
              "validators": {},
              "errors": {
                "required": "Floater Coverage Within is required."
              },
              "visibleWhen": { "field": "floater", "value": "Yes" },
              "validatorsWhen": { "required": true },
              "optionsKey": "floaterCoverageOptions",
              "events": [
                { "name": "change", "action": "onFloaterCoverageWithinChange" }
              ]
            },
            {
              "type": "select",
              "name": "floater_state",
              "label": "State",
              "placeholder": "Select state",
              "errors": {
                "required": "State is required."
              },
              "visibleWhen": {
                "field": "floater_coverage_in",
                "value": "State"
              },
              "validatorsWhen": { "required": true },
              "optionsKey": "stateOptions",
              "events": [{ "name": "change", "action": "onFloaterStateChange" }]
            },
            {
              "type": "input",
              "name": "eq_zone",
              "label": "Earth quake zone",
              "placeholder": "Enter earth quake zone",
              "readonly": true,
              "visibleWhen": { "field": "floater", "value": "Yes" },
              "validatorsWhen": { "required": false },
              "errors": {
                "required": "Pincode is required."
              }
            }
          ]
        },
        {
          "title": "Risk Location",
          "type": "form-array",
          "name": "risk_location",
          "isHideTitles": true,
          "isHideRemoveButton": false,
          "addButtonLabel": "Add Risk Details",
          "limit": 1,
          "visibleWhen": {
            "field": "floater",
            "value": "No"
          },
          "formGroupTemplate": [
            {
              "type": "input",
              "name": "pincode",
              "label": "Pincode",
              "placeholder": "Enter pincode",
              "mask": "PINCODE",
              "validators": {
                "required": true,
                "minLength": 6
              },
              "errors": {
                "required": "Pincode is required.",
                "minlength": "Invalid pincode"
              },
              "events": [{ "name": "change", "action": "fetchLocationDetails" }]
            },
            {
              "type": "select",
              "name": "location",
              "label": "Location",
              "placeholder": "Select location",
              "validators": {
                "required": true
              },
              "errors": {
                "required": "Location is required."
              },
              "optionsKey": "locationOptions",
              "events": [{ "name": "change", "action": "onRiskLocationChange" }]
            },
            {
              "type": "input",
              "name": "address_1",
              "label": "Address 1",
              "placeholder": "Enter address 1",
              "validators": {
                "required": true,
                "maxLength": 180
              },
              "errors": {
                "required": "Address 1 is required.",
                "maxlength": "Address length 1 cannot exceed 180 characters."
              }
            },
            {
              "type": "input",
              "name": "address_2",
              "label": "Address 2",
              "placeholder": "Enter address 2",
              "validators": {
                "required": true,
                "maxLength": 180
              },
              "errors": {
                "required": "Address 2 is required.",
                "maxlength": "Address length 2 cannot exceed 180 characters."
              }
            },
            {
              "type": "input",
              "name": "ed_zone",
              "label": "EQ Zone",
              "placeholder": "Enter EQ zone",
              "readonly": true
            }
          ]
        },
        {
          "title": "Machinery Details",
          "type": "form-array",
          "name": "machinery",
          "addButtonLabel": "Add Machinery Details",
          "limit": 1,
          "isHideAddButton": true,
          "isHideTitles": true,
          "isHideRemoveButton": true,
          "initialGroups": 1,
          "formGroupTemplate": [
            {
              "type": "input",
              "name": "location",
              "readonly": true,
              "label": "Location",
              "placeholder": "Enter location",
              "validators": {
                "required": true
              },
              "errors": {
                "required": "Location is required."
              }
            },
            {
              "type": "select",
              "name": "type_machinery",
              "label": "Machinery",
              "placeholder": "Select machinery",
              "optionsKey": "machineryTypeOptions"
            },
            {
              "type": "input",
              "name": "machinery_description",
              "label": "Description",
              "placeholder": "Enter description"
            },
            {
              "type": "input",
              "name": "make",
              "label": "Make",
              "placeholder": "Enter make",
              "validators": {
                "required": true
              },
              "errors": {
                "required": "Make is required."
              }
            },
            {
              "type": "input",
              "name": "model",
              "label": "Model",
              "placeholder": "Enter model",
              "validators": {
                "required": true
              },
              "errors": {
                "required": "Model is required."
              }
            },
            {
              "type": "input",
              "name": "slno",
              "label": "Serial/Chassis No",
              "placeholder": "Enter serial no",
              "validators": {
                "required": true
              },
              "errors": {
                "required": "Serial/Chassis No is required."
              }
            },
            {
              "type": "input",
              "name": "capacity",
              "label": "Capacity",
              "mask": "NUMERIC",
              "visibleWhen": {
                "field": "proposition",
                "notValue": "Schwing Stetter"
              },
              "placeholder": "Enter capacity"
            },
            {
              "type": "input",
              "inputType": "text",
              "name": "yom",
              "label": "Year of Manufacturing",
              "placeholder": "Enter year of manufacturing",
              "mask": "YYYY",
              "validators": {
                "required": true
              },
              "errors": {
                "required": "Year of Manufacturing is required."
              }
            },
            {
              "type": "input",
              "inputType": "text",
              "name": "cost_of_machinery_si",
              "label": "Cost of Machinery",
              "placeholder": "Enter cost of machinery",
              "mask": "NUMERIC",
              "validators": {
                "required": true
              },
              "errors": {
                "required": "Cost of Machinery is required.",
                "min": "Cost of Machinery cannot be less than {min}.",
                "max": "Cost of Machinery cannot be more than {max}."
              }
            },
            {
              "type": "select",
              "name": "voluntary_excess",
              "label": "Voluntary Excess",
              "placeholder": "Select voluntary excess",
              "optionsKey": "voluntaryExcessOptions"
            }
          ]
        },
        {
          "title": "Premium Detariff",
          "fields": [
            {
              "type": "input",
              "inputType": "text",
              "name": "less_detariff",
              "label": "Less : De-Tariff Discount (%)",
              "placeholder": "Enter discount",
              "validators": {
                "required": true
              },
              "events": [{ "name": "change", "action": "onDetariffChange" }],
              "errors": {
                "required": "De-Tariff Discount is required."
              }
            }
          ]
        },
        {
          "title": "Policy Addons",
          "fields": [
            {
              "type": "select",
              "name": "earthquake_req",
              "label": "Earthquake Required",
              "placeholder": "Select earthquake required",
              "options": ["Yes", "No"]
            },
            {
              "type": "select",
              "name": "terrorism_req",
              "label": "Terrorism Cover",
              "placeholder": "Select terrorism cover",
              "options": ["Yes", "No"],
              "errors": {
                "required": "Terrorism Cover is required."
              }
            }
          ]
        },
        {
          "title": "Claim Experience Details",
          "fields": [
            {
              "type": "select",
              "name": "claimExp",
              "label": "Claim in past 48 months",
              "placeholder": "Select claim in past 48 months",
              "value": "No",
              "options": ["Yes", "No"]
            }
          ]
        },
        {
          "title": "Claim Exp",
          "type": "form-array",
          "name": "claim-exp",
          "isHideTitles": true,
          "isHideRemoveButton": false,
          "addButtonLabel": "Add Claim Exp Details",
          "limit": 1,
          "visibleWhen": {
            "field": "claimExp",
            "value": "Yes"
          },
          "formGroupTemplate": [
            {
              "type": "select",
              "name": "claimLocation",
              "label": "Claim Location",
              "forceRow": true,
              "placeholder": "Select claim location",
              "validators": {
                "required": true
              },
              "errors": {
                "required": "Claim location is required."
              },
              "optionsKey": "claimLocationOptions"
            },
            {
              "type": "input",
              "inputType": "text",
              "name": "claim1Amount",
              "label": "Claim 1 - amount",
              "placeholder": "Enter claim 1 amount",
              "mask": "NUMERIC"
            },
            {
              "type": "input",
              "inputType": "text",
              "name": "year1",
              "label": "Year 1",
              "placeholder": "Enter year 1",
              "mask": "YYYY"
            },
            {
              "type": "input",
              "inputType": "text",
              "name": "claim1Premium",
              "label": "Claim 1 - premium",
              "placeholder": "Enter claim 1 premium",
              "mask": "NUMERIC"
            },
            {
              "type": "input",
              "inputType": "text",
              "name": "claim2Amount",
              "label": "Claim 2 - amount",
              "placeholder": "Enter claim 2 amount",
              "mask": "NUMERIC"
            },
            {
              "type": "input",
              "inputType": "text",
              "name": "year2",
              "label": "Year 2",
              "placeholder": "Enter year 2",
              "mask": "YYYY"
            },
            {
              "type": "input",
              "inputType": "text",
              "name": "claim2Premium",
              "label": "Claim 2 - premium",
              "placeholder": "Enter claim 2 premium",
              "mask": "NUMERIC"
            },
            {
              "type": "input",
              "inputType": "text",
              "name": "claim3Amount",
              "label": "Claim 3 - amount",
              "placeholder": "Enter claim 3 amount",
              "mask": "NUMERIC"
            },
            {
              "type": "input",
              "inputType": "text",
              "name": "year3",
              "label": "Year 3",
              "placeholder": "Enter year 3",
              "mask": "YYYY"
            },
            {
              "type": "input",
              "inputType": "text",
              "name": "claim3Premium",
              "label": "Claim 3 - premium",
              "placeholder": "Enter claim 3 premium",
              "mask": "NUMERIC"
            },
            {
              "type": "input",
              "inputType": "text",
              "name": "claim4Amount",
              "label": "Claim 4 - amount",
              "placeholder": "Enter claim 4 amount",
              "mask": "NUMERIC"
            },
            {
              "type": "input",
              "inputType": "text",
              "name": "year4",
              "label": "Year 4",
              "placeholder": "Enter year 4",
              "mask": "YYYY"
            },
            {
              "type": "input",
              "inputType": "text",
              "name": "claim4Premium",
              "label": "Claim 4 - premium",
              "placeholder": "Enter claim 4 premium",
              "mask": "NUMERIC"
            }
          ]
        },
        {
          "title": "Coverage Options",
          "fields": [
            {
              "type": "select",
              "name": "package_plan",
              "label": "Optional Cover",
              "optionsKey": "packagePlanOptions",
              "placeholder": "Select optional cover",
              "events": [{ "name": "change", "action": "onPackagePlanChange" }]
            }
          ],
          "actions": [
            {
              "type": "button",
              "label": "Add Covers",
              "name": "addCoversBtn",
              "action": "addCovers",
              "iconPath": "https://d2fciuteqrodiu.cloudfront.net/common-style/assets/images/icon-add.svg"
            }
          ]
        },
        {
          "title": "Addons/Covers",
          "fields": [
            {
              "type": "toggle",
              "name": "addon_marine",
              "label": "Marine",
              "visibleWhen": {
                "field": "package_plan",
                "value": "Marine"
              },
              "validatorsWhen": { "required": true },
              "dependentFields": ["addon_marine_premium"]
            },
            {
              "type": "select",
              "name": "marine_selection",
              "label": "Coverage Type",
              "placeholder": "Select coverage type",
              "options": ["All Risk", "Basic Cover"],
              "value": "All Risk",
              "clearWhenHidden": false,
              "visibleWhen": {
                "or": [
                  {
                    "and": [
                      { "field": "addon_marine", "value": true },
                      { "field": "proposition", "value": "SF" }
                    ]
                  },
                  {
                    "and": [
                      { "field": "addon_marine", "value": true },
                      { "field": "proposition", "value": "AAR" }
                    ]
                  },
                  {
                    "and": [
                      { "field": "addon_marine", "value": true },
                      { "field": "proposition", "value": "Xperitus" }
                    ]
                  }
                ]
              },
              "validators": {
                "required": true
              },
              "errors": {
                "required": "Coverage Type is required."
              }
            },
            {
              "type": "static",
              "name": "addon_marine_premium",
              "hidden": true
            },
            {
              "type": "toggle",
              "name": "GPA",
              "label": "GPA",
              "hidden": true
            }
          ]
        },
        {
          "title": "Manual Referral",
          "fields": [
            {
              "type": "select",
              "name": "manual_referral",
              "label": "Underwriter Referral Needed for This Quote?",
              "placeholder": "Select an option",
              "value": "No",
              "options": ["Yes", "No"]
            },
            {
              "type": "input",
              "inputType": "text",
              "name": "underwriterRemarks",
              "label": "Underwriter Remarks",
              "placeholder": "Enter remarks",
              "visibleWhen": {
                "field": "manual_referral",
                "value": "Yes"
              },
              "validatorsWhen": { "required": true },
              "errors": {
                "required": "Underwriter Remarks is required."
              }
            }
          ]
        }
      ]
    }
  ],
  "viewBreakup": {
    "title": "Premium Summary",
    "items": [
      { "label": "Premium Excluding Terrorism", "tag": "gross_temp" },
      { "label": "Terrorism Premium", "tag": "terrorism_temp" },
      { "label": "Gross Premium", "tag": "premium_value" },
      { "label": "IGST 18%:", "tag": "total_tax" }
    ],
    "total": "total_premium"
  },
  "offCanvasConfigs": {
    "title": "Add Covers",
    "searchPlaceholder": "Search Covers",
    "doneButtonLabel": "Done",
    "fields": [
      {
        "name": "thirdPartyLiability",
        "label": "Third Party Liability",
        "subFields": [
          {
            "type": "input",
            "inputType": "text",
            "name": "addon_si",
            "label": "Customize SI",
            "placeholder": "Enter customize SI"
          },
          {
            "type": "input",
            "inputType": "text",
            "name": "derived_add_si",
            "label": "Max SI limit",
            "placeholder": "Enter Max SI limit",
            "readonly": true
          },
          {
            "type": "select",
